{
  "name": "compare-tokenizers",
  "private": true,
  "version": "0.1.0",
  "description": "A test suite comparing Node.js BPE tokenizers for use with AI models.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/compare-tokenizers",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del build",
    "prebuild": "run-s clean",
    "predev": "run-s clean",
    "pretest": "run-s build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "run-p test:*",
    "test:unit": "node build/index.mjs",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.16",
    "gpt-3-encoder": "^1.1.4",
    "gpt-tokenizer": "^2.4.0",
    "gpt3-tokenizer": "^1.1.5",
    "js-tiktoken": "^1.0.14",
    "markdown-table": "^3.0.3",
    "tiktoken": "^1.0.16",
    "tiktoken-node": "^0.0.7",
    "tinybench": "^2.9.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.5.5",
    "del-cli": "^5.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "openai",
    "compare-tokenizers",
    "chat",
    "encoder",
    "bpe",
    "gpt",
    "gpt-3",
    "gpt3",
    "gpt4",
    "chatbot",
    "machine learning",
    "conversation",
    "conversational ai",
    "ai",
    "ml",
    "bot"
  ]
}
